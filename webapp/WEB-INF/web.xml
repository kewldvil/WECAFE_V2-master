	<session-config>
		<session-timeout>1000</session-timeout>
		<cookie-config>
			<http-only>true</http-only>
		</cookie-config>
		<tracking-mode>COOKIE</tracking-mode>
	</session-config>

	<!-- <error-page> <error-code>500</error-code> <location>/error/500</location> 
